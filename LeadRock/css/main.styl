.overlay
    display: none
    width: 100%
    height: 100%
    position: fixed
    top: 0
    z-index: 990
    background-color: #000000
    opacity: 0.6

.popup
    display: none
    position: fixed
    top: 22%;
    left: calc(50% - 125rem)
    z-index 999
    background: #7603E8
    mix-blend-mode: normal
    box-shadow: 0rem 0rem 10rem rgba(0, 0, 0, 0.25)
    border-radius: 15rem
    width: 300rem
    height: 450rem
    flex-direction: column
    justify-content: space-between
    align-items: center
    padding: 27rem 16rem
    opacity: 1 
    & .close-button
        position: absolute
        right: 16rem
        top: 16rem
        width: 10rem
        height: 10rem
        background: url('../img/close-btn.svg') no-repeat
        background-size: cover
    &-main
        display: flex
        flex-direction: column
        justify-content: center
        align-items: center
        width: 255rem
        &__image
            width: 162rem
            height: 162rem
            & img
                border-radius: 50%
        &__title
            margin: 20rem 0 0 0
            font-size: 30rem
            line-height: 34rem
            letter-spacing: 0.05em
            color: #55FF00
            font-family: 'tt_commonsblack', sans-serif
            text-align: center
        &__paragraph
            font-size: 15rem
            line-height: 17rem
            text-align: center
            letter-spacing: 0.1em
            color: #FFFFFF
            margin-top: 5rem
    &-statistic
