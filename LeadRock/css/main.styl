html
    box-sizing: border-box
    -ms-overflow-style: scrollbar
    font-family: 'tt_commonslight', sans-serif
    font-size: 0.09765625vw

body
    margin: 0
    font-size: 16rem
    color: #333333

.container
    margin-right: auto
    margin-left: auto
    overflow-x: hidden

img
    width: 100%
    height: auto

svg
    width: 100%
    height: auto
    max-height: 99rem

*,::after,::before
    box-sizing: inherit

.header
    display: flex
    flex-direction: column
    justify-content: space-between
    align-items: flex-end
    padding: 74rem 87.15rem
    min-height: 100vh
    background: url('../img/header-back.jpg') no-repeat
    background-size: cover
    &-nav
        display: flex
        justify-content: flex-end
        &__logo
            width: 291.79rem
            height: 57.78rem
    &-title
        position: relative
        display: inline-block
        margin: 0 51.58rem 0 0
        max-width: 491.42rem
        font-family: 'tt_commonsblack', sans-serif
        font-size: 100rem
        line-height: 115rem
        letter-spacing: 0.2em
        text-transform: uppercase
        color: #fff
        &:after
            content: ''
            position: absolute
            top: 38rem
            right: -65rem
            width: 206rem
            height: 126rem
            background: url('../img/header-arrow-2.svg') no-repeat
            background-size: contain
    &-button
        position: relative
        bottom: 30rem
        display: flex
        justify-content: center
        width: 100%
        &__link
            position: relative
            z-index: 10
            display: flex
            justify-content: center
            align-items: center
            min-width: 231rem
            min-height: 62.91rem
            box-shadow: 0rem 0rem 4rem #4EFF01
            border-radius: 40rem
            font-size: 20rem
            letter-spacing: 0.5em
            background: #55FF00
            font-family: 'tt_commonsblack', sans-serif
            color: #1A1A1A
            text-decoration: none
            text-transform: uppercase
            &:before
                content: ''
                position: absolute
                z-index: -1
                top: -190rem
                left: -220rem
                width: 210rem
                height: 232rem
                background: url('../img/header-arrow-1.svg') no-repeat
                background-size: cover
            &:after
                content: ''
                position: absolute
                z-index: -1
                top: 16rem
                left: 213rem
                width: 333.5rem
                height: 150.96rem
                background: url('../img/header-arrow-3.svg') no-repeat
                background-size: cover
            &-span
                position: relative
                left: 7rem
                top: 3rem

